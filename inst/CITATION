citHeader("To cite rTRM in publications use:")

citEntry(entry="Article",
	title = "rTRMui: a shiny interface for the identification of transcriptional regulatory modules",
	author = "Diego Diez, Diego Miranda-Saavedra",
	year = "2013, submitted",
	journal = "Journal",
	volume = "",
	number = "",
	pages = "",
	doi = "",
	url = "",

	textVersion =
	paste("rTRMui: an R package for the identification of transcriptional regulatory modules", "Diego Diez, Diego Miranda-Saavedra,",
	"2013, submitted")
)

citFooter("")
